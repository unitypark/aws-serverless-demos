{"ast":null,"code":"import{useState,useEffect}from'react';function useOpenSearch(query){const[data,setData]=useState(null);const fetchData=async()=>{await fetch(\"\".concat(process.env.REACT_APP_API_DOMAIN,\"/search?query=\").concat(query)).then(response=>response.json()).then(result=>{setData(result.data);}).catch(err=>{console.log('error',err);});};useEffect(()=>{console.log('query: ',query);fetchData();},[query]);return{data};}export default useOpenSearch;","map":{"version":3,"names":["useState","useEffect","useOpenSearch","query","data","setData","fetchData","fetch","concat","process","env","REACT_APP_API_DOMAIN","then","response","json","result","catch","err","console","log"],"sources":["/Users/junghwpark/Dev/Projects/AWS/aws-serverless-demos/serverless-web-hosting/apigw-lambda-vpc-opensearch/website/src/useOpenSearch.js"],"sourcesContent":["import { useState, useEffect } from 'react';\n\nfunction useOpenSearch(query) {\n  const [data, setData] = useState(null);\n\n  const fetchData = async () => {\n    await fetch(\n      `${process.env.REACT_APP_API_DOMAIN}/search?query=${query}`\n    )\n      .then((response) => response.json())\n      .then((result) => {\n        setData(result.data);\n      })\n      .catch((err) => {\n        console.log('error', err);\n      });\n  };\n\n  useEffect(() => {\n    console.log('query: ', query);\n    fetchData();\n  }, [query]);\n\n  return { data };\n}\n\nexport default useOpenSearch;\n"],"mappings":"AAAA,OAASA,QAAQ,CAAEC,SAAS,KAAQ,OAAO,CAE3C,QAAS,CAAAC,aAAaA,CAACC,KAAK,CAAE,CAC5B,KAAM,CAACC,IAAI,CAAEC,OAAO,CAAC,CAAGL,QAAQ,CAAC,IAAI,CAAC,CAEtC,KAAM,CAAAM,SAAS,CAAG,KAAAA,CAAA,GAAY,CAC5B,KAAM,CAAAC,KAAK,IAAAC,MAAA,CACNC,OAAO,CAACC,GAAG,CAACC,oBAAoB,mBAAAH,MAAA,CAAiBL,KAAK,CAC3D,CAAC,CACES,IAAI,CAAEC,QAAQ,EAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,CAAC,CACnCF,IAAI,CAAEG,MAAM,EAAK,CAChBV,OAAO,CAACU,MAAM,CAACX,IAAI,CAAC,CACtB,CAAC,CAAC,CACDY,KAAK,CAAEC,GAAG,EAAK,CACdC,OAAO,CAACC,GAAG,CAAC,OAAO,CAAEF,GAAG,CAAC,CAC3B,CAAC,CAAC,CACN,CAAC,CAE