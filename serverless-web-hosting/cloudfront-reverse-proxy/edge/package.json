{
  "name": "cloudfront-edge-lambdas",
  "version": "1.0.0",
  "scripts": {
    "pre-build": "node ./node_modules/del-cli/cli.js \"./dist\" && npm run copy:package:files && cd ./dist && npm install --production",
    "build": "npm run pre-build && npm run compile",
    "compile": "node ./node_modules/typescript/bin/tsc --build ./tsconfig.json",
    "copy:package:files": "node ./node_modules/copyfiles/copyfiles -f \"./package*.json\" ./dist/ && node ./node_modules/copyfiles/copyfiles -f \"./.npmignore\" ./dist/",
    "lint": "eslint .",
    "prettier": "prettier --check './**/*.{ts,tsx,json,md}'",
    "prettier-fix": "npm run prettier -- --write",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.572.0",
    "@aws-sdk/client-ssm": "^3.554.0",
    "aws-jwt-verify": "^2.1.1",
    "aws-lambda": "^1.0.7",
    "axios": "^1.6.5",
    "pino": "^8.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/aws-lambda": "^8.10.89",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "aws-sdk": "^2.815.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "dotenv-webpack": "^8.0.1",
    "esbuild": "^0.20.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "jest": "^27.4.7",
    "prettier": "^3.2.5",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.4.2",
    "typescript": "^4.5.4",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "aws",
    "cognito",
    "userpool",
    "cloudfront",
    "lambda",
    "edge",
    "private",
    "website"
  ]
}
